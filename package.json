{
  "name": "focus4",
  "version": "1.6.0-rc1",
  "description": "Focus v4 (Typescript + MobX)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/get-focus/focus4.git"
  },
  "author": "damien.frikha@kleegroup.com",
  "license": "MIT",
  "homepage": "https://github.com/get-focus/focus4",
  "scripts": {
    "prepublish": "in-publish && tsc --outDir ./ && node copy-css.js ./ || true",
    "postpublish": "rm -rf index.js index.d.ts ioc.js ioc.d.ts reference.js reference.d.ts user.js user.d.ts util.js util.d.ts && rm -rf application entity list message network router search testing translation",
    "start": "tsc && node copy-css.js dist",
    "stylelint": "stylelint src/**/*.css",
    "tcm": "tcm src",
    "tcm:watch": "tcm -w src",
    "test": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && ts-node ./node_modules/tape/bin/tape src/**/__test__/index.* | tap-spec",
    "testgen": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && ts-node src/testing/generator.ts src ..",
    "tslint": "tslint --project tsconfig.json --type-check --force"
  },
  "dependencies": {
    "@types/core-decorators": "0.10.32",
    "@types/glob": "5.0.30",
    "@types/i18next": "2.3.38",
    "@types/lodash": "4.14.64",
    "@types/numeral": "0.0.20",
    "@types/react": "15.0.1",
    "@types/react-addons-test-utils": "0.14.18",
    "@types/react-dom": "15.5.0",
    "@types/tape": "4.2.30",
    "@types/uuid": "2.0.29",
    "core-decorators": "0.18.0",
    "focus-components": "3.3.2",
    "i18next": "8.1.0",
    "ignore-styles": "5.0.1",
    "inversify": "4.1.0",
    "inversify-inject-decorators": "3.0.1",
    "lodash": "4.17.4",
    "material-design-icons-iconfont": "3.0.3",
    "material-design-lite": "1.2.1",
    "mobx": "3.1.9",
    "mobx-react": "4.1.8",
    "moment": "2.18.1",
    "numeral": "2.0.6",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-css-themr": "2.0.0",
    "react-dom": "15.5.4",
    "reflect-metadata": "0.1.10",
    "tape": "4.6.3",
    "tslib": "1.7.0",
    "uuid": "3.0.1",
    "yester": "0.10.0-dev.13"
  },
  "devDependencies": {
    "@types/shelljs": "0.7.1",
    "in-publish": "2.0.0",
    "shelljs": "0.7.7",
    "stylelint": "7.10.1",
    "tap-spec": "4.1.1",
    "ts-node": "3.0.4",
    "tslint": "5.2.0",
    "typed-css-modules": "0.2.0",
    "typescript": "2.4.0-dev.20170504"
  }
}
